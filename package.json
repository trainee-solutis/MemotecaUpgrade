{
  "name": "memoteca",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all --parallel ng-serve backend-json-server backend-index",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "ng-serve": "ng serve",
    "backend-json-server": "cd backend/json-server && npm start >nul 2>&1",
    "backend-index": "cd backend/email-sender && npm start >nul 2>&1",
    "install-all": "npm install && npm-run-all install-backend-json-server install-backend-email-sender",
    "install-backend-json-server": "npm install --prefix backend/json-server",
    "install-backend-email-sender": "npm install --prefix backend/email-sender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@auth0/angular-jwt": "^5.1.2",
    "bcrypt": "^5.1.1",
    "jose": "^4.15.2",
    "rxjs": "~7.5.0",
    "stream": "^0.0.2",
    "tslib": "^2.6.2",
    "util": "^0.12.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.3",
    "@angular/cli": "~14.0.3",
    "@angular/compiler-cli": "^14.0.0",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.2"
  }
}
